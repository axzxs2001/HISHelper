        }
        //查询产品(查询全部)
        function QueryAllProducts() {
            $.ajax({
                type: "GET",
                url: '/queryallproducts',
                data: {},
                success: function (dataBack) {
                    var str = '';
                    str += '<thead>';
                    str += '<tr>';
                    str += '<th><span></span>标题</th>';
                    str += ' <th><span></span>优先级</th>';
