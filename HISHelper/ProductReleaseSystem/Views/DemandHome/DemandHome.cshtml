                    str += '</thead>';
                    $(dataBack.data).each(function (index, item) {
                        str += '<thead id="' + item.ID + '" value="' + item.ID + '">';
                        str += '<tr>';
                        str += '<th style="font-size:16px;" id="a' + item.ID + '">' + item.产品名 + '</th>';
                        str += '<th></th>';
                        str += '<th></th>';
                        str += '<th></th>';
